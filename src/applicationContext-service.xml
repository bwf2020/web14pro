<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
   http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd
   ">


<bean name="dog1" class="day17.Dog"></bean>

	<bean name="user1" class="day17.User1">
	
		<property name="name">
			<value>BWF</value>
		</property>
		
		<property name="list">
			<list>
				<value>1</value>
				<value>2</value>
				<value>3</value>
				<value>4</value>
				<value>5</value>
				<value>3</value>
			</list>
		</property>
		<property name="set">
		
			<set>
				<value>a</value>
				<value>b</value>
				<value>c</value>
				<value>b</value>
			</set>
		
		</property>
		<property name="map">
		
			<map>
				<entry key="a" value="A"></entry>
				<entry key="b" value-ref="dog1"></entry>
				<entry>
					<key>
						<value>c</value>
					</key>
					<value>C</value>
				</entry>
				<entry>
					<key>
						<value>d</value>
					</key>
					<ref  bean="dog1"/>
				</entry>
			</map>
			
		</property>
		
		<property name="pro">
		
			<props>
				<prop key="a">A</prop>
				<prop key="b">B</prop>
				<prop key="c">C</prop>
			</props>
		
		</property>
		
		<property name="array">
		
			<array>
				<value>1</value>
				<value>2</value>
				<value>3</value>
				<value>4</value>
			</array>
		
		</property>
	
	</bean>
	
</beans>